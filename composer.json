{
    "name": "ananiaslitz/hyperf-avro",
    "description": "Avro integration for Hyperf framework with AOP support",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "php",
        "hyperf",
        "avro",
        "serialization",
        "aop"
    ],
    "autoload": {
        "psr-4": {
            "Ananiaslitz\\HyperfAvro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "hyperf/di": "^3.0",
        "hyperf/contract": "^3.0",
        "apache/avro": "^1.12",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/finder": "^6.0 || ^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "swoole/ide-helper": "^4.5"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "Ananiaslitz\\HyperfAvro\\ConfigProvider"
        }
    }
}